{
  "name": "Flutter Toolbox",
  "version": "1.0",
  "description": "Flutter Toolbox for DartPad",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "options_page": "tool/options.html",
  "background": {
    "scripts": [
      "tool/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "build/web/index.html",
    "default_icon": {
      "16": "assets/images/get_started16.png",
      "32": "assets/images/get_started32.png",
      "48": "assets/images/get_started48.png",
      "128": "assets/images/get_started128.png"
    }
  },
  "icons": {
    "16": "assets/images/get_started16.png",
    "32": "assets/images/get_started32.png",
    "48": "assets/images/get_started48.png",
    "128": "assets/images/get_started128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://dartpad.dartlang.org/*"
      ],
      "js": [
        "tool/script.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://dartpad.dev/embed-flutter.html*"
      ],
      "js": [
        "tool/flutteride.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}